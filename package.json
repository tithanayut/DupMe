{
  "name": "dupme",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently -c auto --names CLIENT,SERVER npm:dev:frontend npm:dev:backend",
    "dev:frontend": "vite dev",
    "dev:backend": "nodemon --watch ./src/server --exec node ./src/server/server.js",
    "build": "vite build",
    "start": "node ./src/server/server.js",
    "postinstall": "husky install"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.1",
    "eslint": "^8.49.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "nodemon": "^3.0.1",
    "postcss": "^8.4.30",
    "prettier": "^3.0.3",
    "tailwindcss": "^3.3.3",
    "vite": "^4.4.5"
  },
  "dependencies": {
    "@socket.io/admin-ui": "^0.5.1",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "node-global-key-listener": "^0.1.1",
    "socket.io": "^4.7.2",
    "socket.io-client": "^4.7.2",
    "sweetalert2": "^11.7.32"
  }
}
